<resources>
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>

        <!--custom font-family 적용-->
        <item name="android:textViewStyle">@style/customTextViewFontStyle</item>
        <item name="android:buttonStyle">@style/customButtonFontStyle</item>
        <item name="android:editTextStyle">@style/customEditTextFontStyle</item>
        <item name="android:radioButtonStyle">@style/customRadioButtonFontStyle</item>
        <item name="android:checkboxStyle">@style/customCheckboxFontStyle</item>
    </style>

    <!-- Custom font 적용 -->
    <style name="customTextViewFontStyle" parent="@android:style/Widget.DeviceDefault.TextView">
        <item name="android:fontFamily">@font/custom_font_family</item>
    </style>
    <style name="customButtonFontStyle" parent="@android:style/Widget.DeviceDefault.Button.Borderless">
        <item name="android:fontFamily">@font/custom_font_family</item>
    </style>
    <style name="customEditTextFontStyle" parent="@android:style/Widget.DeviceDefault.EditText">
        <item name="android:fontFamily">@font/custom_font_family</item>
    </style>
    <style name="customRadioButtonFontStyle" parent="@android:style/Widget.DeviceDefault.CompoundButton.RadioButton">
        <item name="android:fontFamily">@font/custom_font_family</item>
    </style>
    <style name="customCheckboxFontStyle" parent="@android:style/Widget.DeviceDefault.CompoundButton.CheckBox">
        <item name="android:fontFamily">@font/custom_font_family</item>
    </style>


    <!--  Text Style -->
    <!-- Title style -->
    <style name="title_style">
    <item name="android:layout_width">wrap_content</item>
    <item name="android:layout_height">wrap_content</item>
        <item name="android:text">운동하자</item>
        <item name="android:textSize">50dp</item>
        <item name="android:fontFamily">@font/typo_ssangmundongb</item>
        <item name="android:textColor">#ffffff</item>
    </style>

    <style name="relative_writing_edit">
        <item name="android:background">@drawable/edit_background</item>
        <item name="android:editable">false</item>
        <item name="android:textColor">#000000</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:layout_width">110dp</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginTop">10dp</item>
    </style>



    <style name="relative_writing_spinner">
        <item name="android:background">@drawable/spinner_background</item>
        <item name="android:layout_width">110dp</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:padding">5dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginTop">10dp</item>
    </style>

    <style name="relative_writing_tv">
        <item name="android:background">@drawable/edit_background</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">17dp</item>
        <item name="android:textAlignment">center</item>
        <item name="android:paddingRight">8dp</item>
        <item name="android:paddingTop">12dp</item>
        <item name="android:layout_width">250dp</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:layout_margin">10dp</item>
    </style>

    <style name="relative_writing_tv2">
        <item name="android:background">@drawable/edit_background</item>
        <item name="android:textColor">#000000</item>
        <item name="android:padding">5dp</item>
        <item name="android:textSize">15dp</item>
        <item name="android:layout_width">400dp</item>
        <item name="android:layout_height">280dp</item>
        <item name="android:layout_marginTop">10dp</item>
    </style>

    <!-- Sub Title style -->
    <style name="text_style">
        <item name ="android:layout_width">wrap_content</item>
        <item name ="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:textSize">23dp</item>
        <item name="android:fontFamily">@font/nanumsquare_acb</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="alarm_text_style">
        <item name ="android:layout_width">wrap_content</item>
        <item name ="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">25dp</item>
        <item name="android:textSize">18dp</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="mypage_text_style">
        <item name ="android:layout_width">match_parent</item>
        <item name ="android:layout_height">100dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:background">@color/viewBackground</item>
        <item name="android:textSize">18dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#000000</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:gravity">center_vertical</item>
    </style>
    <!-- Text style -->

    <!-- Line style -->
    <style name="view_style">
        <item name ="android:layout_width">match_parent</item>
        <item name ="android:layout_height">2dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name = "android:background">@color/line</item>
    </style>
    <!-- Line style -->

    <!-- Button style -->

    <!-- Menu Button style -->
    <style name="menuBtn_style">
        <item name="android:background">@drawable/button_background</item>
        <item name="android:layout_width">180dp</item>
        <item name="android:fontFamily">@font/nanumsquare_acb</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textStyle">bold</item>
        <item name="android:padding">5dp</item>
    </style>

    <!-- Relative Board style -->
    <style name="relative_btn">
        <item name="android:background">@drawable/button_background</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:fontFamily">@font/nanumsquare_acb</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_width">76dp</item>
        <item name="android:layout_margin">5dp</item>
    </style>

    <!-- relative Matching alarm style -->
    <style name="relative_btn2">
        <item name="android:background">@drawable/button_background</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:fontFamily">@font/nanumsquare_acb</item>
        <item name="android:layout_width">126dp</item>
        <item name="android:layout_height">47dp</item>
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:layout_marginTop">9dp</item>
    </style>
    <!-- Button style -->

    <!-- Spinner style -->
    <style name="spinner_style">
        <item name="android:background">@drawable/spinner_background</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingTop">7dp</item>
        <item name="android:layout_margin">7dp</item>
        <item name ="android:layout_width">80dp</item>
        <item name ="android:layout_height">40dp</item>
    </style>

    <!-- Edit style -->
    <style name="edit_style">
        <item name="android:layout_width">220dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:background">@drawable/edit_background</item>
        <item name="android:layout_marginTop">7dp</item>
    </style>

    <!-- Recycler style -->
    <!-- Recycler Text style(안 데이터) -->
    <style name="recycler_text_style">
        <item name ="android:layout_width">wrap_content</item>
        <item name ="android:layout_height">wrap_content</item>
        <item name="android:textSize">15dp</item>
        <item name="android:fontFamily">@font/nanumsquarer</item>
        <item name="android:textColor">#000000</item>
    </style>

    <!-- Recycler style(겉 틀) -->
    <style name="recycler_style">
        <item name ="android:layout_width">match_parent</item>
        <item name ="android:layout_height">455dp</item>
        <item name ="android:layout_marginRight">5dp</item>
        <item name ="android:layout_marginLeft">5dp</item>
        <item name ="android:scrollbars">vertical</item>
    </style>

    <!-- place Recycler style(city, gu) -->
    <style name="place_recycler_style">
        <item name ="android:layout_width">180dp</item>
        <item name ="android:layout_height">300dp</item>
        <item name ="android:padding">2dp</item>
        <item name ="android:layout_marginTop">16dp</item>
        <item name ="android:layout_marginLeft">16dp</item>
        <item name ="android:background">@drawable/recycler_background</item>

    </style>
    <!-- Recycler style -->

    <!-- bottomNavigation style -->
    <style name ="bottomNavigation_style">
        <item name ="android:layout_width">match_parent</item>
        <item name ="android:layout_height">wrap_content</item>
    </style>

    <style name="table_style">
        <item name ="android:layout_width">match_parent</item>
        <item name ="android:layout_height">30dp</item>
        <item name="android:background">@drawable/border_background</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:fontFamily">@font/nanumsquare_acb</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:layout_marginLeft">5dp</item>
    </style>

    <style name="table_text_style">
        <item name ="android:layout_width">wrap_content</item>
        <item name ="android:layout_height">wrap_content</item>
        <item name ="android:textAlignment">center</item>
        <item name="android:padding">7dp</item>
        <item name="android:textSize">13dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffff</item>
    </style>
ㄱㅁ
    <style name="table_title_style">
        <item name ="android:layout_width">wrap_content</item>
        <item name ="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">13dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffff</item>
    </style>

    <style name="table_text_style2">
        <item name ="android:layout_width">wrap_content</item>
        <item name ="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_weight">1.5</item>
        <item name="android:textSize">15dp</item>
        <item name="android:textColor">#000000</item>
    </style>

    <!-- Dialog style -->
    <style name="dialog_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:background">@color/title</item>
        <item name="android:gravity">center</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">20dp</item>
        <item name="android:textColor">#ffffff</item>
    </style>

    <style name="dialog_button_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:background">@color/title</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">16dp</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:text">확인</item>
    </style>
    <!-- Dialog style -->

    <style name="radio_button_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">16dp</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:text">확인</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="switch_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginTop">25dp</item>
        <item name="android:textSize">18dp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:thumb">@drawable/switch_thumb</item>
    </style>
</resources>
